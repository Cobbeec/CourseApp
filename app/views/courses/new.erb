<h1>Create a New Course</h1>

<form action="/courses" method="POST">
  <label>Name:</label>
  <br></br>
  <input type="text" name="course[name]" id="course_name">

  <br></br>
  <label>Subject:</label>
  <br></br>
  <input type="text" name="course[subject]" id="course_subject">

  <br></br>
  <label>Teacher:</label>
  <br></br>
  <input type="text" name="course[teacher]" id="course_teacher">

  <input type="submit" value="Create Course">

</form>
